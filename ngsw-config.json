{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "navigationUrls": [],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
  ],
  "dataGroups": [
    {
      "name": "API",
      "urls": ["http://localhost:3333", "https://api.rtravel.fr"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "12h",
        "timeout": "5s"
      }
    }
  ]
}
